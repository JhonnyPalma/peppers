net: "train.prototxt"
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 700
display: 20
max_iter: 4000
momentum: 0.9
weight_decay: 0.0005
snapshot: 400
snapshot_prefix: "caffe"
solver_mode: CPU
type: "Nesterov"
